{
	"name": "obsidian-s3-sync-and-backup",
	"version": "1.0.0",
	"description": "Bi-directional vault synchronization and scheduled backups for Obsidian using S3-compatible storage (AWS S3, MinIO, Cloudflare R2) with optional end-to-end encryption",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "eslint .",
		"test": "jest",
		"test:unit": "jest --testPathIgnorePatterns='integration'",
		"test:integration": "node --env-file=.env node_modules/.bin/jest --config jest.integration.config.cjs",
		"test:all": "npm run test:unit && npm run test:integration",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"version": "node scripts/version.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"s3",
		"sync",
		"backup",
		"encryption"
	],
	"author": "Ceil√£oLabs",
	"license": "MIT",
	"devDependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@eslint/js": "9.30.1",
		"@types/jest": "^29.5.12",
		"@types/node": "^16.11.6",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jiti": "2.6.1",
		"ts-jest": "^29.1.2",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.700.0",
		"@smithy/protocol-http": "^5.3.7",
		"@smithy/types": "^4.11.0",
		"hash-wasm": "^4.11.0",
		"idb": "^8.0.1",
		"obsidian": "latest",
		"tweetnacl": "^1.0.3"
	}
}